{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch server.js via nodemon",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "nodemon",
        "program": "${workspaceRoot}/server.js",
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
    }]
}